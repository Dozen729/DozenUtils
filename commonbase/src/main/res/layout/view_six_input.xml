<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:gravity="center_horizontal"
    android:orientation="horizontal">

    <com.dozen.commonbase.view.roundview.RoundLinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/six_ll_limit"
        app:rv_cornerRadius="2dp"
        app:rv_backgroundColor="#FFC6C6C6">
        <com.dozen.commonbase.view.sixinput.LastInputEditText
            android:id="@+id/et_one"
            android:layout_width="@dimen/six_input_size"
            android:layout_height="@dimen/six_input_size"
            android:layout_margin="@dimen/six_input_limit" />
    </com.dozen.commonbase.view.roundview.RoundLinearLayout>


    <com.dozen.commonbase.view.roundview.RoundLinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/six_ll_limit"
        app:rv_cornerRadius="2dp"
        app:rv_backgroundColor="#FFC6C6C6">
        <com.dozen.commonbase.view.sixinput.LastInputEditText
            android:id="@+id/et_two"
            android:layout_width="@dimen/six_input_size"
            android:layout_height="@dimen/six_input_size"
            android:layout_margin="@dimen/six_input_limit" />
    </com.dozen.commonbase.view.roundview.RoundLinearLayout>
    <com.dozen.commonbase.view.roundview.RoundLinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/six_ll_limit"
        app:rv_cornerRadius="2dp"
        app:rv_backgroundColor="#FFC6C6C6">
        <com.dozen.commonbase.view.sixinput.LastInputEditText
            android:id="@+id/et_three"
            android:layout_width="@dimen/six_input_size"
            android:layout_height="@dimen/six_input_size"
            android:layout_margin="@dimen/six_input_limit" />
    </com.dozen.commonbase.view.roundview.RoundLinearLayout>
    <com.dozen.commonbase.view.roundview.RoundLinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/six_ll_limit"
        app:rv_cornerRadius="2dp"
        app:rv_backgroundColor="#FFC6C6C6">
        <com.dozen.commonbase.view.sixinput.LastInputEditText
            android:id="@+id/et_four"
            android:layout_width="@dimen/six_input_size"
            android:layout_height="@dimen/six_input_size"
            android:layout_margin="@dimen/six_input_limit" />
    </com.dozen.commonbase.view.roundview.RoundLinearLayout>
    <com.dozen.commonbase.view.roundview.RoundLinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/six_ll_limit"
        app:rv_cornerRadius="2dp"
        app:rv_backgroundColor="#FFC6C6C6">
        <com.dozen.commonbase.view.sixinput.LastInputEditText
            android:id="@+id/et_five"
            android:layout_width="@dimen/six_input_size"
            android:layout_height="@dimen/six_input_size"
            android:layout_margin="@dimen/six_input_limit"
            android:inputType="number" />
    </com.dozen.commonbase.view.roundview.RoundLinearLayout>
    <com.dozen.commonbase.view.roundview.RoundLinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/six_ll_limit"
        app:rv_cornerRadius="2dp"
        app:rv_backgroundColor="#FFC6C6C6">
        <com.dozen.commonbase.view.sixinput.LastInputEditText
            android:id="@+id/et_six"
            android:layout_width="@dimen/six_input_size"
            android:layout_height="@dimen/six_input_size"
            android:layout_margin="@dimen/six_input_limit" />
    </com.dozen.commonbase.view.roundview.RoundLinearLayout>

</LinearLayout>