<?xml version="1.0" encoding="utf-8"?>
<com.dozen.commonbase.view.roundview.RoundLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:rv_backgroundColor="@color/wheel_white"
    app:rv_cornerRadius="@dimen/wheel_dialog_radius"
    android:layout_margin="10dp"
    android:orientation="vertical">

    <include layout="@layout/wheel_dialog_title_bar" />

    <LinearLayout
        android:id="@+id/wheel_id_picker_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingStart="@dimen/wheel_picker_padding_left"
        android:paddingTop="@dimen/wheel_picker_padding_top"
        android:paddingEnd="@dimen/wheel_picker_padding_right"
        android:paddingBottom="@dimen/wheel_picker_padding_bottom" />

</com.dozen.commonbase.view.roundview.RoundLinearLayout>